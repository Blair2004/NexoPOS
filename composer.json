{
    "name": "blair2004/nexopos",
    "type": "project",
    "description": "The Free Modern Point Of Sale System build with Laravel, TailwindCSS and Vue.js.",
    "keywords": [
        "nexopos",
        "point of sale",
        "pos",
        "pos system"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2.0",
        "brianium/paratest": "^7.1",
        "brick/math": "^0.12.0",
        "dekor/php-array-table": "^2.0",
        "doctrine/dbal": "^3.0",
        "fakerphp/faker": "^1.12",
        "gumlet/php-image-resize": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^11.0",
        "laravel/reverb": "@beta",
        "laravel/sanctum": "^4.0",
        "laravel/telescope": "^5.0",
        "laravel/tinker": "^2.0",
        "nikic/php-parser": "^5.0",
        "phpoffice/phpspreadsheet": "^3.3",
        "picqer/php-barcode-generator": "^3.2",
        "predis/predis": "^2.2",
        "pusher/pusher-php-server": "^7.0",
        "spatie/laravel-db-snapshots": "^2.2",
        "symfony/http-client": "^7.0",
        "symfony/mailgun-mailer": "^7.0",
        "tormjens/eventy": "^0.8",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "laravel/pint": "^1.1.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-install-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('public/storage');\" || artisan storage:link --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php artisan storage:link --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan ns:translate --symlink"
        ]
    }
}
